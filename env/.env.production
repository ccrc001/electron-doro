# 生产环境配置
VITE_APP_ENV=production
# VITE_APP_BASE_API=http://uat.crm.xuexiluxian.cn
# VITE_APP_BASE_API=http://jsonplaceholder.typicode.com

VITE_APP_BASE_API=/api
VITE_APP_UPLOAD_URL=https://api.your-domain.com/upload

# 代理目标地址
# VITE_APP_PROXY_TARGET=http://jsonplaceholder.typicode.com
VITE_APP_PROXY_TARGET=http://localhost:3000

# Mock 配置 - 生产环境也启用 Mock
VITE_USE_MOCK=true
# 开发服务器配置
VITE_APP_PORT=3000

# 是否启用代理
VITE_APP_PROXY=true

# 是否在生产环境开启 sourcemap
VITE_APP_SOURCEMAP=false

# 是否压缩
VITE_APP_COMPRESS=true
